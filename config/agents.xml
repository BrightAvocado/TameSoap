<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	<agent name="reactive-rla">
		<set class-path="bin/"/>
		<set class-name="Reactive"/>
		<set discount-factor=".85"/>
	</agent>
	<agent name="reactive-rla-low_discount">
		<set class-path="bin/"></set>
		<set class-name="Reactive"></set>
		<set discount-factor=".1"></set>
	</agent>
	<agent name="reactive-rla-high_discount">
		<set class-path="bin/"></set>
		<set class-name="Reactive"></set>
		<set discount-factor=".99"></set>
	</agent>	
	<agent name="reactive-rla-medium_discount">
		<set class-path="bin/"></set>
		<set class-name="Reactive"></set>
		<set discount-factor=".5"></set>
	</agent>
</agents>
	
